{
  "name": "javascript-starter-project",
  "version": "1.0.0",
  "description": "JavaScript starter project",
  "scripts": {
    "start": "npm-run-all --parallel webpack-watch-dev dev-server share lint:watch",
    "prod": "./node_modules/.bin/webpack -p --progress --config=config/webpack.production.js --mode production",
    "webpack-watch-dev": "./node_modules/.bin/webpack -p --progress --config=config/webpack.development.js --mode development",
    "dev-server": "webpack-dev-server --open --config=config/webpack.development.js",
    "share": "lt --port 8080",
    "lint": "esw ./config/webpack.config.* app --color",
    "lint:watch": "npm run lint -- --watch"
  },
  "author": "Wesley Smits",
  "license": "MIT",
  "dependencies": {
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.26.0",
    "css-loader": "^2.1.1",
    "eslint": "3.8.1",
    "eslint-plugin-import": "2.0.1",
    "eslint-watch": "2.1.14",
    "localtunnel": "^1.9.1",
    "mini-css-extract-plugin": "^0.7.0",
    "npm-run-all": "^4.1.5",
    "postcss-at-rules-variables": "^0.1.7",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-each": "^0.10.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.6",
    "postcss-nested": "^4.1.2",
    "postcss-preset-env": "^6.6.0",
    "postcss-reporter": "^6.0.1",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.5.0",
    "webpack-merge": "^4.2.1"
  }
}
